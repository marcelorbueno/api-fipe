{
  "version": 2,
  "outputDirectory": "public-minimal",
  "buildCommand": null,
  "installCommand": "echo 'No install needed for minimal version'",
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api-minimal/health"
    },
    {
      "source": "/ping",
      "destination": "/api-minimal/ping"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api-minimal/$1"
    }
  ]
}