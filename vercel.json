{
  "version": 2,
  "outputDirectory": "public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/ping",
      "destination": "/api/ping"
    },
    {
      "source": "/db-test",
      "destination": "/api/db-test"
    },
    {
      "source": "/db-simple",
      "destination": "/api/db-simple"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/auth/$1"
    },
    {
      "source": "/auth-test",
      "destination": "/api/auth-test"
    },
    {
      "source": "/db-tables",
      "destination": "/api/db-tables"
    }
  ]
}