{
  "version": 2,
  "outputDirectory": "public",
  "buildCommand": "prisma generate",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/ping",
      "destination": "/api/ping"
    },
    {
      "source": "/db-test",
      "destination": "/api/db-test"
    }
  ]
}