{
  "version": 2,
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/ping",
      "dest": "/api/ping"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/auth"
    },
    {
      "src": "/fipe/(.*)",
      "dest": "/api/fipe"
    },
    {
      "src": "/users/(.*)",
      "dest": "/api/users"
    },
    {
      "src": "/vehicles/(.*)",
      "dest": "/api/vehicles"
    },
    {
      "src": "/patrimony/(.*)",
      "dest": "/api/patrimony"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}