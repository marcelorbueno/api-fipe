{
  "name": "api-fipe",
  "version": "1.0.0",
  "main": "api/index.ts",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc && npx prisma generate",
    "start": "node dist/server.js",
    "prisma": "prisma",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "seed-full": "ts-node -r tsconfig-paths/register src/scripts/seed-full.ts",
    "_comment_tests": "Scripts de teste atualizados",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPatterns=integration",
    "test:unit": "jest --testPathPatterns=unit",
    "test:auth": "jest --testPathPatterns=auth",
    "test:fipe": "jest --testPathPatterns=fipe",
    "test:health": "jest --testPathPatterns=health",
    "test:patrimony": "jest --testPathPatterns=patrimony",
    "test:vehicles": "jest --testPathPatterns=vehicles",
    "test:users": "jest --testPathPatterns=users",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "test:bail": "jest --bail",
    "_comment_cleanup": "Limpar dados de teste",
    "test:cleanup": "ts-node -r tsconfig-paths/register -e \"require('./src/tests/helpers/auth-helper').cleanupTestData()\"",
    "migrate:test": "DATABASE_URL=postgresql://fipe:senha123@postgres:5432/fipe_test_db npx prisma migrate deploy"
  },
  "dependencies": {
    "@fastify/cors": "8.5.0",
    "@fastify/jwt": "7.2.4",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-jaeger": "^2.1.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.205.0",
    "@opentelemetry/exporter-otlp-http": "^0.26.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/instrumentation-dns": "^0.49.0",
    "@opentelemetry/instrumentation-fastify": "^0.50.0",
    "@opentelemetry/instrumentation-fs": "^0.25.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@prisma/client": "^6.13.0",
    "@scalar/fastify-api-reference": "^1.35.6",
    "axios": "^1.6.8",
    "bcrypt": "^6.0.0",
    "bcryptjs": "2.4.3",
    "dotenv": "16.5.0",
    "fastify": "4.28.1",
    "https-proxy-agent": "^7.0.2",
    "jsonwebtoken": "^9.0.2",
    "pg-connection-string": "^2.9.1",
    "prisma": "^6.13.0",
    "zod": "3.25.32"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "3.0.12",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.4.1",
    "@types/supertest": "^6.0.3",
    "@vercel/node": "^5.3.24",
    "eslint": "^9.27.0",
    "jest": "^30.0.5",
    "jest-html-reporters": "^3.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
