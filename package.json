{
  "name": "api-fipe",
  "version": "1.0.0",
  "description": "API BMC FIPE",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "dev:netlify": "netlify dev",
    "build": "tsc && npx prisma generate",
    "start": "node dist/server.js",
    "postinstall": "prisma generate",
    "prisma": "prisma",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "seed:full": "ts-node -r tsconfig-paths/register src/scripts/seed-full.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPatterns=integration",
    "test:unit": "jest --testPathPatterns=unit",
    "test:auth": "jest --testPathPatterns=auth",
    "test:fipe": "jest --testPathPatterns=fipe",
    "test:health": "jest --testPathPatterns=health",
    "test:patrimony": "jest --testPathPatterns=patrimony",
    "test:vehicles": "jest --testPathPatterns=vehicles",
    "test:users": "jest --testPathPatterns=users",
    "test:verbose": "jest --verbose",
    "test:cleanup": "ts-node -r tsconfig-paths/register -e \"require('./src/tests/helpers/auth-helper').cleanupTestData()\"",
    "migrate:test": "DATABASE_URL=postgresql://fipe:senha123@postgres:5432/fipe_test_db npx prisma migrate deploy"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prisma": "^6.13.0",
    "typescript": "^5.9.3",
    "zod": "^3.25.32"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@netlify/functions": "^4.2.7",
    "@rocketseat/eslint-config": "^2.2.2",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "jest-html-reporters": "^3.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
